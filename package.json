{
  "name": "movies-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "cypress:component":
      "concurrently --kill-signal \"tailwindcss -i ./src/app/globals.css -o ./cypress/globals.css --watch\" \"cypress run --component --browser chrome\"",
