      "concurrently --kill-signal \"tailwindcss -i ./src/app/globals.css -o ./cypress/globals.css --watch\" \"cypress open --component --browser chrome\""
  },
  "dependencies": {
    "@types/node": "20.3.3",
    "@types/react": "18.2.14",
    "@types/react-dom": "18.2.6",
    "autoprefixer": "10.4.14",
    "eslint": "8.44.0",
    "eslint-config-next": "13.4.7",
    "next": "13.4.7",
    "postcss": "8.4.24",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.3.2",
    "typescript": "5.1.6"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "cypress": "^12.17.0"
  }
}
